{
  "name": "marktex-bundle",
  "version": "0.3.5",
  "description": "markdown parser",
  "main": "dist/marktex.js",
  "scripts": {
    "build": "webpack",
    "build-es5": "webpack --config webpack.es5.js",
    "build-min": "webpack --config webpack.min.js",
    "build-es5-min": "webpack --config webpack.es5.min.js",
    "prepublish": "yarn run build && yarn run build-min && yarn run build-es5 && yarn run build-es5-min",
    "t": "./node_modules/.bin/mocha.cmd --reporter spec --require ts-node/register",
    "b": "./node_modules/.bin/mocha.cmd --prof --reporter spec --require ts-node/register",
    "bench": "yarn run b src/**/*.bench.ts",
    "test": "yarn run t src/**/*.test.ts"
  },
  "repository": "github.com/Myriad-Dreamin/marktex",
  "author": "Myriad-Dreamin <camiyoru@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-classes": "^7.9.2",
    "@babel/preset-env": "^7.9.0",
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^7.0.2",
    "babel-loader": "^8.1.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "core-js": "^3.6.4",
    "marktex.js": "^0.3.5"
  }
}
